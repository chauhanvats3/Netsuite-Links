{
    "manifest_version": 2,
    "name": "NetSuite Shortcuts",
    "version": "1.0",
    "description": "Go to popular dev links of NetSuite",
    "permissions": ["tabs"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Open NetSuite Login"
    },
    "background": {
        "page": "bg.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
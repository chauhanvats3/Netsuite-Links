{
    "manifest_version": 2,
    "name": "NetSuite Links",
    "version": "2.2.0",
    "description": "NetSuite Dev Links",
    "permissions": ["tabs", "https://*.app.netsuite.com/*"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "NetSuite Links"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_security_policy": "script-src 'self' https://www.gstatic.com/; object-src 'self'",
    "key": ""
}
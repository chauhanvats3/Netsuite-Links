{
    "manifest_version": 2,
    "name": "NetSuite Links",
    "version": "2.6.0",
    "description": "Load N/* Modules , search via Shortcut and More.",
    "permissions": ["tabs", "https://*.app.netsuite.com/*"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "NetSuite Links"
    },
    "background": {
        "scripts": ["thirdParty/external/jquery/jQuery.js", "messagingBackbone.js","shortcut_handler.js"],
        "persistent": true
    },
    "icons": {
        "16": "icons/NewIconStar16.png",
        "32": "icons/NewIconStar32.png",
        "48": "icons/NewIconStar48.png",
        "128": "icons/NewIconStar128.png"
    },
    "content_security_policy": "script-src 'self' https://www.gstatic.com/; object-src 'self'",
    "key": "",
    "commands": {
        "load-module-shrtct": {
          "suggested_key": {
            "default": "Ctrl+M",
            "mac": "Command+M"
          },
          "description": "Load Modules via Shortcut."
        },
        "search-suite-answers-shrtct": {
          "suggested_key": {
            "default": "Ctrl+Shift+S",
            "mac": "Command+Shift+S"
          },
          "description": "Search SuiteAnswers via Shortcut."
        },
        "_execute_browser_action": {
          "suggested_key": {
            "windows": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Y",
            "chromeos": "Ctrl+Shift+U",
            "linux": "Ctrl+Shift+J"
          }
        },
        "_execute_page_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+E",
            "windows": "Alt+Shift+P",
            "mac": "Alt+Shift+P"
          }
        }
      }
}